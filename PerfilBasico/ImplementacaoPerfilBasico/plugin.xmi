        <childPackages xsi:type="org.eclipse.epf.uma:ContentPackage" xmi:id="_yqUOYHh9EeiLnYC-lUd3Ww" name="fase_um" guid="_yqUOYHh9EeiLnYC-lUd3Ww" presentationName="Fase Um">
